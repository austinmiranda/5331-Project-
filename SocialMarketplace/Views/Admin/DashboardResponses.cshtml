@model SocialMarketplace.Models.ViewModels.Admin.DashboardSeriesViewModel
@{
    ViewBag.Title = "Responses per Day";
    Layout = null;
}

<h2>@ViewBag.Title</h2>

@{
    var myChart = new Chart(width: 600, height: 400)
        .AddTitle("Donation Responses")
        .AddSeries(
            name: "Default",
            xValue: Model.Xvalues,
            yValues: Model.Yvalues)
        .Write();
}